// JS file
// Query Selector - The querySelector() method returns the first element that matches a specified CSS selector(s) in the document.
const list = document.querySelector('ul.collection')
console.log('list',list)
const listitem = document.querySelector('li.collection-item')
console.log('list-item',listitem)
// Nodes -  "Node" is an interface that is implemented by multiple other objects, including "document" and "element". 
const nodes = document.querySelector('ul.collection').childNodes
console.log('Nodes',nodes)
// Type of the node
console.log('node-type',nodes[0].nodeType)
console.log('node-type',nodes[0].nodeName)
console.log('node-type',nodes[1].nodeType)
console.log('node-type',nodes[1].nodeName)
const children = document.querySelector('ul.collection').children
console.log('children',children)
